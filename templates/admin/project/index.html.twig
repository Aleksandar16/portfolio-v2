{% extends 'base.html.twig' %}

{% block title %}Hello ProjectController!{% endblock %}

{% block body %}
    <div>
        <h1>Projets</h1>
        <ul>
            {% for project in projects %}
                <li><a href="{{ path('project_edit', {'slug': project.slug}) }}">{{ project.name }}</a></li>
            {% endfor %}
        </ul>
    </div>
{% endblock %}
